<%= search_form_for @q, url: @url do |f| %>
  <%= f.label :word_or_similarity_cont, '検索' %>
  <div class="input-group">
    <%= f.text_field :word_or_similarity_cont, placeholder: "単語・類似", class: 'form-control' %>
    <span class="input-group-btn">
     <%= f.submit '検索', class: "btn btn-primary" %>
    </span>
  </div>
<% end %>